{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "Webpack react+typescript boilerplate",
  "main": "src/index.tsx",
  "author": "Alexey-Koren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alexey-Koren/webpack-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexey-Koren/webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack serve --open --color --hot --mode=development",
    "prod": "cross-env NODE_ENV=production webpack serve --open --color --hot --mode=production",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -f -r build",
    "analyze": "cross-env analyze=true NODE_ENV=production webpack --mode=production",
    "prettier:format": "prettier --write \"**/*.+(ts|tsx|scss)\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:fix-staged": "eslint --fix",
    "fix": "run-s prettier:format lint:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json $FILE",
    "svgo-all": "cross-env svgo -r -f src --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@types/classnames": "^2.3.1",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "glob-all": "^3.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "mini-css-extract-plugin": "^2.4.4",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-id-generator": "^3.0.2",
    "react-router-dom": "^6.0.1",
    "react-svg-loader": "^3.0.3",
    "react-table": "^7.7.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "url-loader": "^4.1.0",
    "vanilla-lazyload": "^17.5.0",
    "webpack": "^5.62.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
