{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "ignorePatterns": ["build", "dist", "node_modules"],
  "rules": {
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
